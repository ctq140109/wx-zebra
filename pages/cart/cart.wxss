.full {
  position: absolute;
  top: 0;
  width: 100%;
  height: calc(100% - 100rpx);
  display: flex;
  align-items: center;
  justify-content: center;
  /* flex-direction: column; */
}

.full-item {
  width: 240rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.full image {
  width: 240rpx;
  height: 240rpx;
}

.empty {
  color: #e3e3e3;
  font-size: 28rpx;
  margin: 10rpx;
}

.go {
  width: 230rpx;
  height: 50rpx;
  color: #fff;
  background: #ef0007;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 20rpx;
  border-radius: 30rpx;
  font-size: 30rpx;
  padding: 10rpx;
}

.main-bottom {
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  border-top: 2rpx solid #ccc;
  display: flex;
  background: #f5f5f5;
  z-index: 10;
}

.bottom-left {
  flex: 3;
  background: #fff;
  display: flex;
}

.manage-btn {
  width: 120rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  border: 1px solid #999;
  border-radius: 10rpx;
  color: #999;
}

.between {
  justify-content: space-between;
}

.bottom-left1 {
  flex: 1;
  padding: 0 20rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}

.bottom-left2 {
  flex: 1;
  padding: 0 15rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.bottom-right {
  flex: 1;
}

.bottom-right1 {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #fff;
  /* background: #ef0007; */
}

.bottom-right2 {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #707070;
  background: #f5f5f5;
}

.cart-box {
  margin-bottom: 100rpx;
}

.cart-box .item {
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  align-items: center;
  padding: 20rpx;
  background: #fff;
  margin-bottom: 16rpx;
}

.cart-box .item .goods-info {
  margin-left: 20rpx;
  width: calc(100% - 248rpx);
}

.cart-box .goods-img {
  width: 160rpx;
  height: 160rpx;
  margin-left: 20rpx;
  border-radius: 20rpx;
}

.cart-box .row {
  color: #fff;
  display: flex;
  flex-direction: row;
  /* width: 430rpx; *//* width: 100%; */
  justify-content: space-between;
  margin-bottom: 20rpx;
}

.cart-box .goods-name {
  color: #000;
  font-size: 32rpx;
  /* width: calc(100% - 100rpx); */
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.cart-box .goods-price {
  font-size: 32rpx;
  color: #ef0007;
}

.cart-box .goods-type {
  color: #999;
  font-size: 24rpx;
  margin-bottom: 20rpx;
}

.cart-box .num-box {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.cart-box .goods-btn {
  flex: 1;
  /* width: 60rpx; */
  height: 50rpx;
  padding: 0;
  /* line-height: 60rpx; */
  font-weight: bold;
  color: #999;
  margin: 0;
  background: #fff;
}

.cart-box .num {
  flex: 1;
  color: #000;
  /* width: 80rpx; */
  text-align: center;
  height: 50rpx;
  font-size: 24rpx;
  border-right: 1px solid #ccc;
  border-left: 1px solid #ccc;
}

.cart-box .btn-add {
  font-size: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.cart-box .btn-minus {
  font-size: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.cart-box .btn-groups {
  width: 180rpx;
  display: flex;
  flex-direction: row;
  /* justify-content: flex-end; */
  border: 1px solid #ccc;
  /* border-radius: 10rpx; */
}

.cart-fixbar {
  position: fixed;
  bottom: 0;
  background: #333;
  height: 100rpx;
  width: 100%;
  padding: 0 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.cart-fixbar .allSelect {
  display: flex;
  flex-direction: row;
  height: 100rpx;
  align-items: center;
  color: #fff;
  font-size: 32rpx;
}

.cart-fixbar .allSelect-text {
  margin-left: 20rpx;
}

.cart-fixbar .count {
  margin-left: 80rpx;
  color: #fff;
  font-size: 36rpx;
}

.cart-fixbar .order {
  color: #fff;
  height: 100rpx;
  background: #222;
  line-height: 100rpx;
  position: absolute;
  right: 0;
  padding: 0 40rpx;
  font-size: 32rpx;
}

.cart-fixbar .allnum {
  font-size: 24rpx;
}
