.main {
  width: 100%;
  height: 100%;
  position: fixed;
}

.full {
  width: 100%;
  height: 90%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  float: left;
}

.empty {
  color: #e3e3e3;
  font-size: 28rpx;
  margin: 10rpx;
}

.go {
  width: 230rpx;
  height: 50rpx;
  color: #fff;
  background: #ec4444;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 20rpx;
  border-radius: 30rpx;
  font-size: 30rpx;
  padding: 10rpx;
}

.full image {
  width: 240rpx;
  height: 240rpx;
}

.main-bottom {
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  border-top: 2rpx solid #ccc;
  display: flex;
}
.bottom-left{
  flex: 3;
  background: #fff;
  /* border-top: 1rpx solid #ccc; */
}
.bottom-right{
  flex: 1;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
}
.cartList{
  height: 100vh;
}
.items{
  width: 100%;
  height: 100rpx;
  margin-bottom: 10rpx;
  background: #ec4444;
}