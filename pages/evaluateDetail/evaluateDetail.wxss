.goods-wraper {
  background: #fff;
  /* margin-bottom: 20rpx; */
  padding: 20rpx 30rpx;
  border-bottom: 2rpx solid #ccc;
}

.loading-cover {
  margin: 20rpx 0;
  text-align: center;
  font-size: 28rpx;
  color: #acacac;
}

.goods-item {
  display: flex;
  display: -webkit-flex;
  align-items: center;
  padding: 10rpx 0;
}

.has-border {
  border-bottom: 1px solid #ccc;
}

.goods-img {
  width: 160rpx;
  border-radius: 20rpx;
}

.goods-msgbox {
  width: calc(100% - 180rpx);
  height: 180rpx;
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
}

.goods-name-cover {
  flex: 1;
  display: flex;
  align-items: center;
}

.goods-name {
  color: #000;
  font-size: 32rpx;
  overflow: hidden;
  word-break: break-all;
}

.goods-info {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.goods-price {
  font-size: 32rpx;
  color: #ef0007;
}

.goods-type {
  font-size: 28rpx;
  color: #ccc;
}

.goods-num {
  color: #ccc;
}

.blank_box {
  position: fixed;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.evaluate-list .generalComment {
  height: 94rpx;
  padding: 0 30rpx;
  margin-top: 1rpx;
  background-color: #fff;
  font-size: 28rpx;
  color: #808080;
  border-bottom: 1px solid #f9f9f9;
}

.acea-row.row-between-wrapper {
  align-items: center;
  justify-content: space-between;
}

.acea-row {
  display: flex;
  flex-wrap: wrap;
}

.acea-row.row-middle {
  align-items: center;
}

.font-color {
  color: #fc4141 !important;
}

.evaluate-list .generalComment .evaluate {
  margin-right: 7rpx;
}

.start {
  width: 122rpx;
  height: 30rpx;
  background-image: url('data: image/png;base64,');
  background-repeat: no-repeat;
  background-size: 122rpx auto;
}

.start.star5 {
  background-position: 0 3rpx;
}

.start.star4 {
  background-position: 0 -30rpx;
}

.start.star3 {
  background-position: 0 -70rpx;
}

.start.star2 {
  background-position: 0 -105rpx;
}

.start.star1 {
  background-position: 0 -140rpx;
}

.start.star0 {
  background-position: 0 -175rpx;
}

.evaluateWtapper .evaluateItem {
  background-color: #fff;
  padding-bottom: 25rpx;
}

.evaluateWtapper .evaluateItem~.evaluateItem {
  border-top: 1rpx solid #f5f5f5;
}

.evaluateWtapper .evaluateItem .pic-text {
  font-size: 26rpx;
  color: #282828;
  height: 95rpx;
  padding: 0 30rpx;
}

.evaluateWtapper .evaluateItem .pic-text .pictrue {
  width: 56rpx;
  height: 56rpx;
  margin-right: 20rpx;
}

.evaluateWtapper .evaluateItem .pic-text .pictrue image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.evaluateWtapper .evaluateItem .pic-text .name {
  max-width: 450rpx;
  margin-right: 15rpx;
}

.evaluateWtapper .evaluateItem .time {
  font-size: 24rpx;
  color: #82848f;
  padding: 0 30rpx;
}

.evaluateWtapper .evaluateItem .evaluate-infor {
  font-size: 28rpx;
  color: #282828;
  margin-top: 19rpx;
  padding: 0 30rpx;
}

.evaluateWtapper .evaluateItem .imgList {
  padding: 0 30rpx 0 15rpx;
  margin-top: 25rpx;
}

.evaluateWtapper .evaluateItem .imgList .pictrue {
  width: 156rpx;
  height: 156rpx;
  margin: 0 0 15rpx 15rpx;
}

.evaluateWtapper .evaluateItem .imgList .pictrue image {
  width: 100%;
  height: 100%;
}

.evaluateWtapper .evaluateItem .reply {
  font-size: 26rpx;
  color: #454545;
  background-color: #f7f7f7;
  border-radius: 5rpx;
  margin: 20rpx 30rpx 0 30rpx;
  padding: 30rpx;
  position: relative;
}

.evaluateWtapper .evaluateItem .reply::before {
  content: "";
  width: 0;
  height: 0;
  border-left: 0.3rem solid transparent;
  border-right: 0.3rem solid transparent;
  border-bottom: 0.4rem solid #f7f7f7;
  position: absolute;
  top: -0.4rem;
  left: 2rem;
}
